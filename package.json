    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "chai": "4.2.0",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "mocha": "5.2.0",
    "nyc": "13.1.0",
    "sinon": "7.2.2",
    "sinon-chai": "3.3.0",
    "standard-version": "^4.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "uuid": "3.0.1"
  },
  "repository": {
